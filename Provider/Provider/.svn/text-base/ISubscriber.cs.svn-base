using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Provider
{
    public interface ISubscriber<TKey, TValue>
        where TKey : IKey
        where TValue : IValue
    {
        IObservable<TValue> GetUpdates(TKey key);
        IEnumerable<TKey> SubscriptionKeys { get; }
        TValue GetEntry(TKey key);
        IEnumerable<TValue> GetEntries(IEnumerable<TKey> keys);
        void UnSubscribe(TKey key);
    }
}
